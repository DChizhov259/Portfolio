.socials {
  display: flex;
}

.socials__item {
  margin-right: 40px;

  @include desktop {
    margin-right: 3.3vw;
  }

  &:last-child {
    margin-right: 0;
  }
}

.socials__icon {
  fill: $social_icon-color;
  height: 25px;
  width: 25px;

  @include tablets {
    height: 30px;
    width: 30px;
  }
}
